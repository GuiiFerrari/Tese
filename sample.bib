@article{LIANG200922,
title = {Dynamic polarization in the Coulomb breakup of loosely bound 17F},
journal = {Physics Letters B},
volume = {681},
number = {1},
pages = {22-25},
year = {2009},
issn = {0370-2693},
doi = {https://doi.org/10.1016/j.physletb.2009.09.055},
url = {https://www.sciencedirect.com/science/article/pii/S0370269309011472},
author = {J.F. Liang and J.R. Beene and A.L. Caraley and H. Esbensen and A. Galindo-Uribarri and C.J. Gross and P.E. Mueller and K.T. Schmitt and D. Shapira and D.W. Stracener and R.L. Varner},
keywords = {Coulomb dissociation, Radioactive beam, Dynamic polarization},
abstract = {Angular distributions of the Coulomb breakup of radioactive 17F were measured by impinging a 10 MeV/nucleon beam on 208Pb and on 58Ni to study the dynamic polarization effects. The breakup products, oxygen and a proton, were detected in coincidence. First-order perturbation theory significantly overpredicts the breakup cross section for the 208Pb target. Dynamical calculations with a dynamic polarization as the leading order correction were performed. The calculations reproduce the data for 17F on 58Ni but overpredict the breakup of 17F on 208Pb by a factor of two at forward angles.}
}

