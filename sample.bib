@article{KOLATA1989503,
title = {A radioactive beam facility using a large superconducting solenoid},
journal = {Nuclear Instruments and Methods in Physics Research Section B: Beam Interactions with Materials and Atoms},
volume = {40-41},
pages = {503-506},
year = {1989},
issn = {0168-583X},
